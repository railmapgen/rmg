    },
    "license": "GPL-3.0-only",
    "dependencies": {
        "@chakra-ui/react": "^2.2.1",
        "@emotion/react": "^11.9.3",
        "@emotion/styled": "^11.9.3",
        "@railmapgen/rmg-components": "^1.2.0",
        "@railmapgen/rmg-palette-resources": "^0.3.6",
        "@railmapgen/rmg-templates-resources": "^0.1.12",
        "@reduxjs/toolkit": "^1.8.2",
        "ag-grid-community": "^27.3.0",
        "ag-grid-react": "^27.3.0",
        "electron-is-dev": "^2.0.0",
        "framer-motion": "^6.3.11",
        "i18next": "^21.8.10",
        "i18next-browser-languagedetector": "^6.1.4",
        "i18next-http-backend": "^1.4.1",
        "jszip": "^3.10.0",
        "nanoid": "^3.3.4",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-i18next": "^11.17.2",
        "react-icons": "^4.4.0",
        "react-redux": "^8.0.2",
        "react-router-dom": "^6.3.0"
    },
    "devDependencies": {
        "@testing-library/jest-dom": "^5.16.4",
        "@testing-library/react": "^13.3.0",
        "@types/jest": "^27.5.2",
        "@types/node": "^16.11.41",
        "@types/react": "^18.0.14",
        "@types/react-dom": "^18.0.5",
        "@types/react-router-dom": "^5.3.3",
        "@types/redux-mock-store": "^1.0.3",
        "electron": "^17.2.0",
        "electron-builder": "^22.14.13",
        "eslint-plugin-prettier": "^4.0.0",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^27.5.1",
        "prettier": "^2.7.1",
        "react-scripts": "^5.0.1",
        "redux-mock-store": "^1.5.4",
        "typescript": "^4.7.4",
        "wait-on": "^5.3.0"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test --watchAll=false",
        "lint": "eslint ./src",
        "lint:dev": "eslint -c ./.eslintrc.dev.json ./src",
        "lint:fix": "eslint ./src --fix",
        "eject": "react-scripts eject",
        "electron-dev": "concurrently \"npm start\" \"wait-on http://localhost:3000 && electron .\"",
        "electron-package": "npx electron-builder build -c.extraMetadata.main=build/electron.js"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "version": "5.2.15",
    "main": "public/electron.js",
    "build": {
        "publish": [
            {
                "provider": "github",
                "vPrefixedTagName": false
            }
        ],
        "snap": {
            "publish": "github"
        }
    },
    "jest": {
        "transformIgnorePatterns": [
            "/node_modules/(?!(@railmapgen/.*))"
        ],
        "moduleNameMapper": {
            "\\.(css|less)$": "identity-obj-proxy"
        }
    }
}
