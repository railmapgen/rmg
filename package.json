        "url": "https://github.com/railmapgen/rmg/issues"
    },
    "license": "GPL-3.0-only",
    "dependencies": {
        "@chakra-ui/react": "^1.8.8",
        "@emotion/react": "^11.9.0",
        "@emotion/styled": "^11.8.1",
        "@railmapgen/rmg-components": "^0.4.1",
        "@railmapgen/rmg-palette-resources": "^0.3.2",
        "@railmapgen/rmg-templates-resources": "^0.1.11",
        "ag-grid-community": "^27.2.1",
        "ag-grid-react": "^27.2.1",
        "framer-motion": "^6.3.0",
        "i18next": "^21.6.16",
        "i18next-browser-languagedetector": "^6.1.4",
        "i18next-http-backend": "^1.4.0",
        "jszip": "^3.9.1",
        "nanoid": "^3.3.3",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-i18next": "^11.16.7",
        "react-icons": "^4.3.1",
        "react-redux": "^7.2.8",
        "react-router-dom": "^6.3.0",
        "redux": "^4.2.0",
        "redux-thunk": "^2.4.1"
    },
    "devDependencies": {
        "@testing-library/jest-dom": "^5.16.4",
        "@testing-library/react": "^12.1.5",
        "@types/enzyme": "^3.10.12",
        "@types/jest": "^27.4.1",
        "@types/node": "^16.11.27",
        "@types/react": "^17.0.44",
        "@types/react-dom": "^17.0.15",
        "@types/react-router-dom": "^5.3.3",
        "@types/redux-mock-store": "^1.0.3",
        "@wojtekmaj/enzyme-adapter-react-17": "^0.6.7",
        "cross-env": "^7.0.3",
        "electron": "^17.2.0",
        "electron-builder": "^22.14.13",
        "electron-is-dev": "^2.0.0",
        "enzyme": "^3.11.0",
        "eslint-plugin-prettier": "^4.0.0",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^27.5.1",
        "prettier": "^2.6.2",
        "react-scripts": "^5.0.1",
        "redux-mock-store": "^1.5.4",
        "typescript": "^4.6.3",
        "wait-on": "^5.3.0"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "test:no-watch": "cross-env CI=true react-scripts test --env=jsdom",
        "lint": "eslint ./src",
        "lint:dev": "eslint -c ./.eslintrc.dev.json ./src",
        "lint:fix": "eslint ./src --fix",
        "eject": "react-scripts eject",
        "electron-dev": "concurrently \"npm start\" \"wait-on http://localhost:3000 && electron .\"",
        "electron-package": "npx electron-builder build -c.extraMetadata.main=build/electron.js"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "version": "5.0.28",
    "main": "public/electron.js",
    "build": {
        "publish": [
            {
                "provider": "github",
                "vPrefixedTagName": false
            }
        ],
        "snap": {
            "publish": "github"
        }
    },
    "jest": {
        "transformIgnorePatterns": [
            "/node_modules/(?!(@railmapgen/.*))"
        ],
        "moduleNameMapper": {
            "\\.(css|less)$": "identity-obj-proxy"
        }
    }
}
