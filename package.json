{
    "homepage": "https://wongchito.github.io/RailMapGenerator",
    "name": "rail-map-generator",
    "repository": "github:wongchito/RailMapGenerator",
    "bugs": {
        "url": "https://github.com/wongchito/RailMapGenerator/issues"
    },
    "license": "GPL-3.0-only",
    "dependencies": {
        "@material-ui/core": "^4.12.3",
        "@railmapgen/rmg-palette-resources": "^0.3.2",
        "electron-is-dev": "^2.0.0",
        "i18next": "^21.6.14",
        "i18next-browser-languagedetector": "^6.1.4",
        "i18next-http-backend": "^1.4.0",
        "jszip": "^3.8.0",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-i18next": "^11.16.2",
        "react-redux": "^7.2.7",
        "react-router-dom": "^6.2.2",
        "redux": "^4.1.2",
        "redux-thunk": "^2.4.1"
    },
    "devDependencies": {
        "@types/enzyme": "^3.10.11",
        "@types/jest": "^27.4.1",
        "@types/node": "^16.11.26",
        "@types/react": "^17.0.43",
        "@types/react-dom": "^17.0.14",
        "@types/react-router-dom": "^5.3.3",
        "@types/redux-mock-store": "^1.0.3",
        "@wojtekmaj/enzyme-adapter-react-17": "^0.6.7",
        "cross-env": "^7.0.3",
        "electron": "^17.2.0",
        "electron-builder": "^22.14.13",
        "enzyme": "^3.11.0",
        "eslint-plugin-prettier": "^4.0.0",
        "prettier": "^2.6.1",
        "react-scripts": "^5.0.0",
        "redux-mock-store": "^1.5.4",
        "typescript": "^4.6.3",
        "wait-on": "^5.3.0"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "test:no-watch": "cross-env CI=true react-scripts test --env=jsdom",
        "lint": "eslint ./src",
        "lint:dev": "eslint -c ./.eslintrc.dev.json ./src",
        "lint:fix": "eslint ./src --fix",
        "eject": "react-scripts eject",
        "electron-dev": "concurrently \"npm start\" \"wait-on http://localhost:3000 && electron .\"",
        "electron-package": "npx electron-builder build -c.extraMetadata.main=build/electron.js"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "version": "3.12.4",
    "main": "public/electron.js",
    "build": {
        "publish": [
            {
                "provider": "github",
                "vPrefixedTagName": false
            }
        ],
        "snap": {
            "publish": "github"
        }
    },
    "jest": {
        "transformIgnorePatterns": [
            "/node_modules/(?!(@railmapgen/.*))"
        ]
    }
}
